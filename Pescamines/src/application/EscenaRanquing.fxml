<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="root_ranquing" prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RanquingController">
   <children>
      <AnchorPane>
         <children>
            <Label fx:id="capçalera" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="139.0" prefWidth="493.0" text="Rànquin Pescamines" textAlignment="CENTER">
               <font>
                  <Font name="Bernard MT Condensed" size="39.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
				<HBox fx:id="botons" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="281.0" prefWidth="600.0">
					<padding>
						<Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
					</padding>
         <children>
            <TableView fx:id="taulaRanquing" prefHeight="221.0" prefWidth="469.0" style="-fx-background-color: transparent; -fx-border-width: none;">
              <columns>
                <TableColumn id="rPossicio" fx:id="rPossicio" prefWidth="63.0" text="Pos" />
                <TableColumn id="rUsuari" fx:id="rUsuari" prefWidth="168.0" text="Usuari" />
                  <TableColumn id="rDificultad" fx:id="rDificultat" prefWidth="115.0" text="Dificultad" />
                  <TableColumn id="rTemps" fx:id="rTemps" prefWidth="122.0" text="Temps" />
              </columns>
            </TableView>
         </children>
				</HBox>
      <HBox alignment="BOTTOM_CENTER" prefHeight="74.0" prefWidth="600.0">
         <children>
            <Button fx:id="boto_tornar" contentDisplay="CENTER" mnemonicParsing="false" onAction="#tornar" prefHeight="50.0" prefWidth="80.0" text="Tornar">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
